#include<stdio.h>
#include<stdlib.h>
#define null 0.0000000000
int main(){
    int n=0,s=0;
    double x=0, r=0;
    scanf("%d",&n);
    double**a=(double**)malloc(n*sizeof(double*));
    for (int i=0; i<n; i++){
        a[i]=(double*)malloc(n*sizeof(double));
    }
    for (int i=0; i<n; i++){
        for (int j=0; j<n; j++){
            scanf("%lf",&a[i][j]);
        }
    }
    /////////////////////////////////////////////////////////////
    int nul=0;
    for (int i=0; i<n; i++){
        for (int i=0; i<n; i++){
            nul=0;
            for (int j=i; j<n; j++){
                if (a[j][i]!=null) {
                    nul=1;
                    break;
                }
            }
        }
            if (nul){
                if (a[i][i]!=null){
                    for (int j=i+1; j<n;j++){
                        x=a[j][i]/a[i][i];
                        for (int l=i; l<n; l++){
                            a[j][l]=a[j][l]-x*a[i][l];
                        }
                    }
                }
                else {
                    s=i;
                    while ((a[s][i]==null)&&(s<n-1)) s+=1;
                    if (s!=n-1){
                        for (int j=s+1; j<n;j++){
                            x=a[j][i]/a[s][i];
                            for (int l=i; l<n; l++){
                                a[j][l]=a[j][l]-x*a[s][l];
                            }
                        }
                    }
                }
            }
        
    }
    /////////////////////////////////////////////////////////////
    for (int i=0; i<n; i++){
        for (int j=i; j<n; j++){
            if ((a[j][i]!=null)&&(j!=i)){
                for (int k=0; k<n; k++){
                    r=a[j][k]; a[j][k]=a[i][k]; a[i][k]=r;
                }
                break;
            }
            if ((a[j][i]!=null)&&(j==i)) break;
        }
    }
   
    ////////////////////////////////////////////////////////////////////////
    double det=1;
    for (int i=0; i<n; i++){
        det=det*a[i][i];
    }
    if (det==null) det=0;
    //////////////////////////////////////////////////////////////////////////
    printf("Определитель матрицы = %0.3lf",det);
    /*for (int i=0; i<n; i++){
        for (int j=0; j<n; j++){
            printf("%lf ",a[i][j]);
        }
        printf("\n");
    }*/
    return 0;
}
