#include<stdio.h>
#include<stdlib.h>
#define null 0.0000000000
int main(){
    int n=0,s=0;
    double x=0, r=0;
    scanf("%d",&n);
    double**a=(double**)malloc(n*sizeof(double*));
    for (int i=0; i<n; i++){
        a[i]=(double*)malloc((n+1)*sizeof(double));
    }
    for (int i=0; i<n; i++){
        for (int j=0; j<n; j++){
            scanf("%lf",&a[i][j]);
        }
    }
    for (int i=0; i<n; i++){
            scanf("%lf",&a[i][n]);
    }
    /////////////////////////////////////////////////////////////
    for (int i=0; i<n; i++){
                if (a[i][i]!=null){
                    for (int j=i+1; j<n;j++){
                        x=a[j][i]/a[i][i];
                        for (int l=i; l<=n; l++){
                            a[j][l]=a[j][l]-x*a[i][l];
                        }
                    }
                }
                else {
                    s=i;
                    while ((a[s][i]==null)&&(s<n-1)) s+=1;
                    if (s!=n-1){
                        for (int j=s+1; j<n;j++){
                            x=a[j][i]/a[s][i];
                            for (int l=i; l<=n; l++){
                                a[j][l]=a[j][l]-x*a[s][l];
                            }
                        }
                    }
                }
        
    }
    /////////////////////////////////////////////////////////////
    int kol=0;
    for (int i=0; i<n; i++){
        for (int j=i; j<n; j++){
            if ((a[j][i]!=null)&&(j!=i)){
                for (int k=0; k<=n; k++){
                    r=a[j][k]; a[j][k]=a[i][k]; a[i][k]=r;
                    kol+=1;
                }
                break;
            }
            if ((a[j][i]!=null)&&(j==i)) break;
        }
    }
   
    ////////////////////////////////////////////////////////////////////////
    double det=1;
    for (int i=0; i<n; i++){
        det=det*a[i][i];
    }
    if (kol%2==1) det*=-1;
    if (det==null) det=0;
    printf("%0.3lf\n", det);
    for (int i=0; i<n; i++){
        for (int j=0; j<=n; j++){
            printf("%lf ",a[i][j]);
        }
        printf("\n");
    }
    //////////////////////////////////////////////////////////////////////////
    if (det==null) ;
    //////////////////////////////////////////////////////////////////////////
    if (det!=null){
        /*for (int i=n-1; i>=0; i--){
            a[i][i]=a[i][n]/a[i][i];
            for (int h=i-1; i>=0; i--){
                a[h][i]=a[h][i]*a[i][i];
                a[h][n]=a[h][n]-a[h][i];
            }
        }*/
        for (int i=n-1; i>=0; i--){
            for (int j=i-1; j>=0;j--){
                x=a[j][i]/a[i][i];
                for (int l=i; l<=n; l++){
                    a[j][l]=a[j][l]-x*a[i][l];
                }
            }
        }
    }
    //////////////////////////////////////////////////////////////////////////
    for (int i=0; i<n; i++){
        printf("x%d = %0.3lf\n", i+1, a[i][n]/a[i][i]);
    }
    return 0;
}
