#include<stdio.h>
#include<math.h>
int main(){
    int n=0, k=0;
    double x=0;
    double vector[1000][1000]={{0},{0}};
    scanf("%d",&n);
    scanf("%d",&k);
    for (int i=0;i<k;i++){
        for (int j=0; j<n;j++){
            scanf("%lf",&vector[i][j]);
        }
    }
    double m=0;
    if (n>k) m=k;
    else m=n;
    for(int j=0; j<m;j++){
        for(int i=1;i<k;i++){
            for(int s=i;s<k;s++){
                x=vector[s][j]/vector[i-1][j];
                for (int g=j; g<n;g++){
                    vector[s][g]-=vector[i-1][g]*x;
                }
            }
        }
    }
    for (int i=0;i<k;i++){
        for (int j=0; j<n;j++){
            printf("%lf ",vector[i][j]);
        }
        printf("\n");
    }
    return 0;
}
