#include <stdio.h>
int koll(int* a,int* b){
    int kol0a=0,kol0b=0;
    for (int i=0;i<3;i++) if (a[i]==0) kol0a+=1;
    for (int i=0;i<3;i++) if (b[i]==0) kol0b+=1;
    if ((kol0a==3)||(kol0b==3)) return 1;
    if ((kol0a==0)&&(kol0b==0)){
        double x=0,y=0,z=0;
        x=a[0]/(float)b[0];
        y=a[1]/(float)b[1];
        z=a[2]/(float)b[2];
        if ((x==y)&&(y==z)) return 1;
        else return 0;
    }
    if (((a[0]==0)&&(b[0]==0)&&(a[1]==0)&&(b[1]==0))||((a[2]==0)&&(b[2]==0)&&(a[1]==0)&&(b[1]==0))||((a[0]==0)&&(b[0]==0)&&(a[2]==0)&&(b[2]==0))) return 1;
    if ((a[0]==0)&&(b[0]!=0)||(a[1]==0)&&(b[1]!=0)||(a[1]==0)&&(b[1]!=0))  return 0;
    if ((b[0]==0)&&(a[0]!=0)||(b[1]==0)&&(a[1]!=0)||(b[1]==0)&&(a[1]!=0))  return 0;
}

int main (){
    int f = 0;
    int a[3]={0};
    int b[3]={0};
    int c[3]={0};
    printf("Введите координаты вектора a\n");
    for (int i=0;i<3;i++) scanf("%d",&a[i]);
    printf("Введите координаты вектора b\n");
    for (int i=0;i<3;i++) scanf("%d",&b[i]);
    printf("Введите координаты вектора c\n");
    for (int i=0;i<3;i++) scanf("%d",&c[i]);
    if (koll(a,b)==1){
        printf("Векторы a и b коллинеарны\n");
        f+=1;
    } 
    if (koll(b,c)==1){
      printf("Векторы b и c коллинеарны\n");
      f+=1;
    } 
    if (koll(a,c)==1){
      printf("Векторы a и c коллинеарны\n");
      f+=1;
    } 
    int opred=a[0]*b[1]*c[2]+c[0]*b[2]*a[1]+a[2]*b[0]*c[1]-c[0]*b[1]*a[2]-b[0]*a[1]*c[2]-a[0]*b[2]*c[1];
    if (opred>=1) printf("Векторы не компланарны, ранг системы - 3");
    else{
      printf("Векторы компланарны, ");
      if (f==3) printf("ранг системы - 1");
      else printf("ранг системы - 2");
    } 
    return 0;
}
